  "devDependencies": {
    "babel-cli": "^6.16.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-power-assert": "^1.0.0",
    "eslint": "^3.7.1",
    "fast-json-patch": "^1.1.1",
    "json-patch-test-suite": "^1.1.0",
    "mocha": "^3.1.2",
    "npm-run-all": "^3.1.0",
    "nyc": "^8.3.1",
    "power-assert": "^1.4.1",
